{
  "ext": "md",
  "outputPath": "./assets",
  "frontmatter": [
    {
      "name": "title",
      "type": "text",
      "question": "Please enter `title`",
      "placeholder": "title",
      "required": true
    },
    {
      "name": "author",
      "type": "text",
      "question": "Please enter `author`",
      "placeholder": "author",
      "required": true
    },
    {
      "name": "og",
      "type": "object",
      "properties": [
        {
          "name": "image",
          "type": "text",
          "question": "Please enter `og:image`",
          "placeholder": "/public/images/og-image.png",
          "required": true
        },
        {
          "name": "og_image_optout",
          "type": "boolean",
          "question": "Do you want to opt-out `og:image`?",
          "placeholder": "og:description",
          "required": true
        }
      ]
    },
    {
      "name": "languages",
      "type": "multiselect",
      "question": "Please select multiple `languages`",
      "options": [
        "javascript",
        "typescript",
        "react",
        "nextjs",
        "nodejs"
      ],
      "placeholder": "languages",
      "required": true
    },
    {
      "name": "published_at",
      "type": "text",
      "question": "Please enter `published_at`",
      "placeholder": "2024/01/01",
      "required": true
    }
  ]
}
